			{
				ele:":radio",
				datatype:"*"
			},
			{
				ele:"select:lt(2)",
				datatype:"*"
			},
			{
				ele:":checkbox",
				datatype:"*"
			},
			{
				ele:"#cc",
				datatype:"*"
			}
		]);
	}
</script>

