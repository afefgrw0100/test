/* Link 全局链接样式2016.11.1 */
body, button, select, textarea, input, label, option, fieldset, legend{font-family:"Microsoft YaHei";outline: none;color: #666;}
body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, fieldset, legend, input, textarea, button, p, blockquote, th, td{margin: 0;padding: 0;}
html{ font-size:12px;}
textarea{resize: none}
.clear{height:1px;overflow:hidden;clear:both;}
a,img{border:0;}
input{ outline: none;}
em{font-style: normal;}
a{color: #666;text-decoration: none;outline: none; }
a:hover{ color: #ff533e; text-decoration: none; outline: none;}
a:visited,a:active,a:link{text-decoration: none;}
body{ width: 100%; height: 100%;}
li{list-style-type:none;}

.fl{float:left;}
.fr{float:right;}

/*top*/
.main{margin-bottom: 70px; }
.nav{ background: #fff; padding:0px 15px; color: #333; font-size: 14px; overflow: hidden;}
.nav a{ color: #fff;}
.navbar-l{position: absolute; top: 0; left: 15px;}
.icon-back{ position: relative;width: 14px; height: 14px;}
.icon-back::before{ position: absolute; top: 17px; left: 0px; border-bottom: 2px solid #666; border-left: 2px solid #666; width: 12px;
 height: 12px;background: #fff; transform: scaleY(0.8) rotateZ(45deg); content: ''; }
 .navbar-m{text-align: center; line-height: 50px; font-size: 15px;}
 
 
.user .man_bg{ position: relative; padding: 10px; overflow: hidden;}
.user .man_bg .pic{ position: absolute; top: 0; left: 0; width: 100%;}
.user .man_bg .pic img{ width: 100%;}
.user .man_bg .man{ background: #fff; height: 62px; width: 62px; overflow: hidden; border: 1px solid #fff; border-radius: 50%;
position: relative; margin: 0 auto; box-shadow: 0px 2px 4px #333;}
.user .man_bg .man img{ width: 60px; height: 60px;}
.user .man_bg .name{ position: relative; width: 100%; text-align: center; color: #fff;padding-top: 5px; overflow: hidden; font-size: 13px;}
.user .man_bg .name em{ margin-left: 6px; margin-right: 6px; }
.user .man_bg .name em.line{ height: 10px; width: 1px; background: #fff; display:inline-block;}
@media only screen and (min-width: 376px) and (max-width: 640px) {
	.user .man_bg{ padding-top: 20px;}
	.user .man_bg .name{ padding-top: 10px;}
}
@media only screen and (min-width: 321px) and (max-width: 375px) {
	.user .man_bg{ padding-top: 15px;}
	.user .man_bg .name{ padding-top: 10px;}
}
.user .money{ background: #fff; border-bottom: 1px solid #c9c9c9; overflow: hidden; padding:10px 0;}
.user .money ul li{ width: 33.33%; text-align: center; float: left;}
.user .money ul li span{ display: block;}
.user .money ul li span i{ margin-right: 5px;}
.user .money ul li span b{ font-weight: normal;}
.user .money ul li span.num{ color: #33a9e8; font-size: 14px;}
.user .money ul li span.num em{ margin-right: 2px;}
.user .money ul li span.icon{ border-right: 1px solid #ccc;}
.user .money ul li:nth-child(3) span.icon{ border: none;}
.user .item-list{ background: #fff; margin-top: 10px; overflow: hidden; }
.user .item-list ul li{ line-height:40px;border-bottom: 1px solid #dedede; padding: 0px 15px; font-size: 14px; position: relative;}
.user .item-list ul li::after{position: absolute; top: 17px; right: 15px; border-bottom: 1px solid #c0bfbf; border-right: 1px solid #c0bfbf; width: 12px;
 height: 12px;background: #fff; transform: scaleY(0.8) rotateZ(-45deg); content: '';}
.user .item-list ul li a{ color: #666; display: block;}
.user .item-list ul li i{ margin-right: 10px;font-size: 18px;}


/*订单页*/

.order-list{ background: #fff; overflow: hidden; padding: 10px 0; margin-top: 10px;}
.order-list ul li{ border-bottom: 1px solid #e4e4e4; line-height: 40px; padding: 0px 15px; font-size: 13px; overflow: hidden;}
.order-list ul li span{ float: left;}
.order-list ul li span.line{ color: #666; margin: 0px 10px;}
.order-list ul li span.num{ position: relative; padding-right: 20px;}
.order-list ul li span.num::before{ position: absolute; top: 15px; right: 0px; border-bottom: 1px solid #666; 
border-right: 1px solid #666; width: 10px; height: 10px;background: #fff; transform: scaleY(0.8) rotateZ(-45deg); content: ''; }

.order-list ul li em{ float: right;}
.order-list ul li em.i-info{ color: #ff6600;}
.order-list ul li.btn{ border-bottom: none;}
.order-list ul li.btn a{ background: #fff;  float: right; padding: 2px 10px; border: 1px solid #33a9e8; 
border-radius: 5px; margin: 10px 0; line-height: 20px; color: #33a9e8;margin-left: 10px;}

.order-list ul>li:last-child{ border-bottom: none;}

/*订单管理*/
.user .ordermgt .money{ margin-top: 10px; font-size: 13px; border-bottom: none;}
.user .ordermgt .money ul li{ width: 50%; border-right: 1px solid #ccc;}
.user .ordermgt .money ul li:last-child{ border-right: none;}
.user .ordermgt .money ul li .icon{ border-right: none;}
.ordermgt .order-list ul li span.num::before{ display: none;}
.ordermgt .item-list ul li i{ color:#33a9e8;}
.ordermgt .item-list ul li .apply i{ color: #f8c158; font-size: 20px; position: relative; top: 3px;}
.ordermgt .item-list ul li .apply em{  line-height: 40px;}
.user .ordermgt .money ul li:first-child span.num{ color: #ff6600;}

.recharge{ background: #fff; padding: 15px; padding-bottom: 40px;}
.recharge .btn-warning{ text-align: center;}
.recharge .num{ margin-top: 10px; overflow: hidden;}
.recharge .num b{ font-weight: normal;}
.recharge .num span{ margin-right: 10px;}
.recharge .btn-warning a{ margin-top: 20px; padding: 6px 50px; background: #f0ad4e; border-radius: 4px; display: inline-block;color: #fff;}
.recharge .input-container input{ font-size: 14px; width: 70%; padding-left: 10px; border: 1px solid #d7d7d7; background: #fff; height: 35px;
 line-height: 35px; border-radius: 3px;}
/*底部*/
.bottom{ position: fixed; bottom: 0; left: 0; width: 100%;}
.bottom ul li{ float: left; width: 33.33%; text-align: center; background: #fff; height:70px; line-height: 60px;}
.bottom ul li a{ color: #666;}
.bottom ul li a.active{ color: #52acf2;}
.bottom ul li i{ display: block; font-size: 24px; line-height: 30px; padding-top: 10px;}
.bottom ul li:nth-child(3) i{ font-size: 22px;}
.bottom ul li b{ display: block; font-size: 14px; font-weight: normal; line-height:20px; padding-bottom: 10px;} 